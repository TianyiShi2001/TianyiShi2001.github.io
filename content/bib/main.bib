	keywords = {Photosynthesis, Light-harvesting, Non-photochemical quenching, Electron microscopy, Crystallisation, Membrane protein},
	note = {Structures and Mechanisms in Molecular Bioenergetics},
	number = {6},
	pages = {753 - 772},
	title = {Crystallisation, structure and function of plant light-harvesting Complex II},
