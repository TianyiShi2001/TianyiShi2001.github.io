@article{Sternberg-1998,
	abstract = {Genetic analysis of the RAS function in Caenorhabditis elegans has not only clarified the functional relationship of signal transduction proteins, but also led to the discovery of new proteins involved positively or negatively in RAS signaling. The stereotyped development of C. elegans has allowed many of the functions of RAS to be elucidated at the level of fates of individual cells.},
