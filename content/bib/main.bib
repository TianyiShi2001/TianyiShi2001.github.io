	doi = {10.1038/ncomms8053},
	id = {van Zon2015},
	isbn = {2041-1723},
	journal = {Nature Communications},
	number = {1},
	pages = {7053},
	title = {Cells change their sensitivity to an EGF morphogen gradient to control EGF-induced gene expression},
	ty = {JOUR},
