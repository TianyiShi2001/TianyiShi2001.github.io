	issn = {0032-0889},
	journal = {Plant Physiology},
	number = {3},
	pages = {1436--1448},
	publisher = {American Society of Plant Biologists},
	title = {NADPH Thioredoxin Reductase C and Thioredoxins Act Concertedly in Seedling Development},
	url = {http://www.plantphysiol.org/content/174/3/1436},
	volume = {174},
	year = {2017},
