	doi = {10.1038/oby.2008.678},
	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1038/oby.2008.678},
	journal = {Obesity},
	number = {7},
	pages = {1326-1331},
	title = {Effects of Insulin on Ketogenesis Following Fasting in Lean and Obese Men},
