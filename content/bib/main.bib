	keywords = {Lipolysis; Fat stores; Triacylglycerol; Lipase; Neutral lipid storage disease},
	number = {1},
	pages = {14--27},
	title = {Lipolysis --A highly regulated multi-enzyme complex mediates the catabolism of cellular fat stores},
	ty = {JOUR},
