	doi = {https://doi.org/10.1016/j.bpj.2017.08.006},
	isbn = {0006-3495},
	journal = {Biophysical Journal},
	number = {7},
	pages = {1531--1539},
	title = {Determination of Cell Membrane Capacitance and Conductance via Optically Induced Electrokinetics},
	ty = {JOUR},
	url = {http://www.sciencedirect.com/science/article/pii/S0006349517308639},
	volume = {113},
