	journal = {Annual Review of Microbiology},
	note = {PMID: 28697666},
	number = {1},
	pages = {393-411},
	title = {Bacterial Cell Division: Nonmodels Poised to Take the Spotlight},
	url = {https://doi.org/10.1146/annurev-micro-102215-095657},
