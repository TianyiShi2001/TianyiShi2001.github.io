	eprint = {https://physoc.onlinelibrary.wiley.com/doi/pdf/10.1113/jphysiol.2011.224972},
	journal = {The Journal of Physiology},
	number = {5},
	pages = {1069-1076},
	title = {Regulation of glucose and glycogen metabolism during and after exercise},
	url = {https://physoc.onlinelibrary.wiley.com/doi/abs/10.1113/jphysiol.2011.224972},
	volume = {590},
	year = {2012},
	bdsk-url-1 = {https://physoc.onlinelibrary.wiley.com/doi/abs/10.1113/jphysiol.2011.224972},
	bdsk-url-2 = {https://doi.org/10.1113/jphysiol.2011.224972}}

