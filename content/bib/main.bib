	j2 = {Adv Exp Med Biol},
	journal = {Advances in experimental medicine and biology},
	keywords = {Ab initio shape reconstruction; Ensemble description of flexible proteins; Hybrid methods in structural biology; Intrinsically disordered proteins; Rigid body modelling; Small-angle X-ray scattering; Solution scattering; Intrinsically Disordered Proteins/*chemistry; Protein Conformation; *Scattering, Small Angle; *X-Ray Diffraction},
	la = {eng},
	pages = {261--289},
	title = {Application of SAXS for the Structural Characterization of IDPs},
	ty = {JOUR},
